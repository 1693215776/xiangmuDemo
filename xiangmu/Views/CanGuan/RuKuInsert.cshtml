
@{
    ViewBag.Title = "RuKuInsert";
    Layout = "~/Views/mubanye/ym.cshtml";
}

<form @Url.Action("RuKuInsert")>
    <div class="container" style=" width:1200px; margin-left:290px">
        <div class="row">
            <div class="col-md-4">
                <label>入库单编号</label>
                <input name="Name" type="text" class="form-control" readonly />
            </div>
            <div class="col-md-4">
                <label>入库单类型</label>

                <div class="controls">

                    <select id="ddlInType" name="ddlInType" class="span12" style="width:350px;height:37px">
                        <option value="">请选择入库单类型</option>
                        <option value="1">采购收货入库</option>
                        <option value="2">销售退货入库</option>
                        <option value="3" selected="selected">生产产品入库</option>
                        <option value="4">领用退还入库</option>
                        <option value="5">借货入库</option>
                        <option value="6">借出还入</option>
                    </select>
                    <span class="help-block"></span>

                </div>
            </div>
            <div class="col-md-4">
                <label>关联订单号</label>
                <input name="Money" type="text" class="form-control" />
            </div>
            <div class="col-md-4">
                <label>制单人</label>
                <input name="Color" type="text" class="form-control" />
            </div>
            <div class="col-md-4">
                <label>供应商编号</label>
                <input name="Name" type="text" class="form-control" />
            </div>
            <div class="col-md-4">
                <label>供应商名称</label>
                <input name="Brand" type="text" class="form-control" />
            </div>
            <div class="col-md-4">
                <label>供应商联系人</label>
                <input name="Money" type="text" class="form-control" />
            </div>
            <div class="col-md-4">
                <label>供应商联系方式</label>
                <input name="Color" type="text" class="form-control" />
            </div>
            <div class="col-md-4">
                <label>制单时间</label>
                <input name="Money" type="text" class="form-control" />
            </div>
            <div class="col-md-4">
                <label>备注</label>
                <input name="Color" type="text" class="form-control" />
            </div>
        </div>
        <div class="row">
            <div class="col-md-8 text-right">
                <button type="submit" class="btn btn-success">添加</button>
                <a class="btn btn-primary" href="@Url.Action("CarQuery")">返回列表</a>
            </div>
        </div>
    </div>
</form>

