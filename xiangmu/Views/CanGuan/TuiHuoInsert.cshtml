
@{
    ViewBag.Title = "TuiHuoInsert";
    Layout = "~/Views/mubanye/ym.cshtml";
}

<head>
    <link href="~/yangshi/bootstrap-3.3.7-dist/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
</head>

<form>
    <div class="row show-grid" style="width:1600px;margin-left:290px">
        <div class=" col-md-3">
            <label for="validationDefault01">退货单号</label>
            <input type="text" class="form-control" id="validationDefault02" value="可不填,自动生成" readonly>
        </div>
        <div class="form-group col-md-3 ">
            <label for="inputState">退货类型</label>
            <select id="inputState" class="form-control">
                <option selected>死的</option>
                <option>...</option>
            </select>
        </div>
        <div class="col-md-3 ">
            <label for="validationDefault01">关联订单号</label>
            <input type="text" class="form-control" id="validationDefault02" y>
        </div>
        <div class="col-md-3">
            <label for="validationDefault02">制单日期</label>
            <input type="date" class="form-control" id="validationDefault02" required>
        </div>
        <div class="col-md-3 ">
            <label for="validationDefault01">客户编号</label>
            <input type="text" class="form-control" id="validationDefault02" value="输入关键字搜索" readonly>
        </div>
        <div class="col-md-3 ">
            <label for="validationDefault01">客户名称</label>
            <input type="text" class="form-control" id="validationDefault02" readonly>
        </div>
        <div class="form-group col-md-3 ">
            <label for="inputState">客户地址</label>
            <input type="text" class="form-control" id="validationDefault02" readonly>
        </div>
        <div class="col-md-3">
            <label for="validationDefault01">客户联系方式</label>
            <input type="text" class="form-control" id="validationDefault02" readonly>
        </div>
        <div class="col-md-4 ">
            <label for="validationDefault01">联系人</label>
            <input type="text" class="form-control" id="validationDefault02" readonly>
        </div>
        <div class="col-md-4">
            <label for="validationDefault01">制单人</label>
            <input type="text" class="form-control" id="validationDefault02" >
        </div>
        <div class="col-md-4">
            <label for="validationDefault01">备注</label>
            <input type="text" class="form-control" id="validationDefault02" >
        </div>
    </div>
</form>
<div class="row">
    <div class="col-md-8 text-right">
        <button type="button" class="btn btn-primary" style="margin-top:30px;">添加</button>
        <button type="button" class="btn btn-secondary" href="@Url.Action("CarQuery")" style="margin-right:100px;margin-top:30px;">返回列表</button>
    </div>
</div>



