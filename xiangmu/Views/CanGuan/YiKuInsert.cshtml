
@{
    ViewBag.Title = "YiKuInsert";
    Layout = "~/Views/mubanye/ym.cshtml";
}

<head>
    <link href="~/yangshi/bootstrap-3.3.7-dist/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>

</head>
<form style="margin-left:300px;">
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="inputEmail4">移库单编号</label>
            <input type="email" class="form-control" id="inputEmail4">
        </div>
        <div class="form-group col-md-4">
            <label for="inputState">移库类型</label>
            <select id="inputState" class="form-control">
                <option selected>死的</option>
                <option>...</option>
            </select>
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="inputCity">关联订单号</label>
            <input type="text" class="form-control" id="inputCity">
        </div>
        <div class="form-group col-md-2">
            <label for="inputZip">制单时间</label>
            <input type="date" class="form-control" id="inputZip">
        </div>
        <div class="form-group col-md-2">
            <label for="inputZip">制单人</label>
            <input type="text" class="form-control" id="inputZip">
        </div>
        <div class="col-sm-10">
            <label for="inputCity">备注</label>
            <input type="text" class="form-control" id="inputPassword3">
        </div>
    </div>
</form>
<div class="row">
    <div class="col-md-8 text-right">
        <button type="button" class="btn btn-primary" style="margin-top:30px;">添加</button>
        <button type="button" class="btn btn-secondary" href="@Url.Action("CarQuery")" style="margin-right:200px;margin-top:30px;">返回列表</button>
    </div>
</div>

