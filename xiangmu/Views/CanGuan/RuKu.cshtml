<script type="text/javascript">
     $(function () {
         kuweichaxun()
     });
     function ss(id) {
         document.getElementById('light').style.display = 'block'; document.getElementById('fade').style.display = 'block';

         $.ajax({
             url: "@Url.Action("chaxun")",
             data: { inid: id },
              dataType:"json",
             type: "get",
             success: function (result) {
                
                 
                 $.each(result.qq, function (inde, value) {
                     alert(value.Inid);
                     $("#ruk").text(value.Inid);
                
                 })
             }
             }) 
              }
     
    
      function kuweichaxun() {

         //库位查询
        $.ajax({
            url: "@Url.Action("lzycha")",
            data: {},
            dataType: "json",
            type: "get",
            success: function (result) {

                $("#tabInfo tr:gt(0)").remove();

                $.each(result.qq, function (inde, value) {

                    var str = "<tr class='widget-title'>";
                    str += "<th><input type='checkbox' style='width: 30px;margin-left:0px;' class='group-checkable' name='check2' value='"+value.Inid+"'/></th>"
                    str += "<th style='width:195px;margin-left:5px;'>" + value.Inid + "</th>";
                    str += "<th style='width:195px;'>" + value.typeid + "</th>";
                    str += "<th style='width:195px;'>" + value.supplierid + "</th>";
                    str += "<th style='width:195px;'>" + value.stateid + "</th>";
                    str += "<th style='width:195px;'>" + value.supname + "</th>";
                    str += "<th>"
                    str += "<a class='icon-remove' href='javascript:void(0)'onclick='ss("+value.Inid+")'><span class='iconfont'>&#xe686;</span></a>&nbsp;&nbsp;&nbsp;</th> ";

                    str += "</tr>";
                    $("#tabInfo").append(str);

                });


            }
        });
    }//库位查询
   

</script>

